FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type                   heRhoThermo;
    mixture                pureMixture;
    transport              polynomial;
    thermo                 hPolynomial;
    equationOfState        icoPolynomial;
    specie                 specie;
    energy                 sensibleEnthalpy;
}

mixture
{
    // coefficients for water

    specie
    {
        nMoles           1;
        molWeight        18;
    }

    equationOfState
    {
        rhoCoeffs<8>        ( -4220.8  19.222  -0.0185  0  0  0  0  0 );
    }

    thermodynamics
    {
        Hf                 0;
        Sf                 0;
        CpCoeffs<8>        ( 5458.3  0  0  0  0  0  0  0 );
    }

    transport
    {
        muCoeffs<8>           ( 0.00008851  0  0  0  0  0  0  0 );
        kappaCoeffs<8>        ( 0.56    0  0  0  0  0  0  0 );
    }
}


